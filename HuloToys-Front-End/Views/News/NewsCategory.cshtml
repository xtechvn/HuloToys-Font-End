@using HuloToys_Front_End.Models.News;
@model List<GetCategoryResponse>
@if(Model != null)
{
    <li> <a onclick="_new.getNewsByTag(1,10,10)" href="tin-tuc" class="cat-tag active tag_10">Tất cả</a></li>
    foreach(var item in Model)
    {
        <li> <a onclick="_new.getNewsByTag(1,10,@item.id)" href="@item.url_path" class="cat-tag tag_@item.id">@item.name</a></li>
    }
}
