@using BIOLIFE.ViewModels;
@model ArticleViewModel

<div class="col-md-3 col-sm-6">
    <h4 class="title">@Html.Raw(Model.category_detail == null ? "" : Model.category_detail.name)</h4>
    <ul class="links">
        @foreach (var item in Model.obj_article_list)
        {
            <li><a href="#">@Html.Raw(item.title)</a></li>
        }
    </ul>
</div>